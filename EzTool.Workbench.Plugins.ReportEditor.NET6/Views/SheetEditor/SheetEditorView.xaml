<UserControl x:Class="EzTool.Workbench.Plugins.ReportEditor.NET6.Views.SheetEditor.SheetEditorView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EzTool.Workbench.Plugins.ReportEditor.NET6.Views.SheetEditor"
             mc:Ignorable="d" Background="LightGray"
             MinHeight="200" MinWidth="500"
             d:DesignHeight="200" d:DesignWidth="500">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <StackPanel Orientation="Vertical" VerticalAlignment="Center">
                <Button Width="124" Height="25" 
                        Command="{Binding RelayCommand}" CommandParameter="ClickButtonEvent" />
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
