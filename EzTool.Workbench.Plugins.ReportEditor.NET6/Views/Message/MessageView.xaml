<UserControl x:Class="EzTool.Workbench.Plugins.ReportEditor.NET6.Views.Message.MessageView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EzTool.Workbench.Plugins.ReportEditor.NET6.Views.Message"
             mc:Ignorable="d" Background="Black"
             Width="200"
             Height="200" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <Button Width="125" Height="35" Content="Close"
                        Command="{Binding RelayCommand}" CommandParameter="MessageViewCloseEvent"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
